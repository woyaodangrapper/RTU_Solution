<Project>
	<Import Project="version.props" />
	<PropertyGroup>
		<!-- 设置目标框架为 .NET 9.0 -->
		<TargetFrameworks>net9.0;net10.0;netstandard2.1</TargetFrameworks>

		<!-- 启用最新的 C# 语言功能 -->
		<LangVersion>preview</LangVersion>

		<!-- 启用可空引用类型 -->
		<Nullable>enable</Nullable>

		<!-- 启用隐式 using 指令 -->
		<ImplicitUsings>enable</ImplicitUsings>

		<!-- 启用服务器垃圾回收 -->
		<ServerGarbageCollection>true</ServerGarbageCollection>

		<!-- 启用并发垃圾回收 -->
		<ConcurrentGarbageCollection>true</ConcurrentGarbageCollection>

		<!-- 设置作者信息 -->
		<Authors>Aspdcs</Authors>

		<!-- 设置产品名称 -->
		<Product>Aspdcs</Product>

		<!-- 设置包前缀 -->
		<PackagePrefix>Aspdcs</PackagePrefix>

		<!-- 设置包图标 -->
		<PackageIcon></PackageIcon>

		<!-- 设置仓库 URL -->
		<RepositoryUrl>https://github.com/woyaodangrapper/Aspdcs</RepositoryUrl>

		<!-- 设置包项目 URL -->
		<PackageProjectUrl>https://github.com/woyaodangrapper/RTU_Solution</PackageProjectUrl>

		<!-- 设置仓库类型 -->
		<RepositoryType>git</RepositoryType>

		<!-- 设置卫星资源语言 -->
		<SatelliteResourceLanguages>zh-Hans</SatelliteResourceLanguages>
		<SatelliteResourceLanguages>zh-Hant</SatelliteResourceLanguages>
		<SatelliteResourceLanguages>en</SatelliteResourceLanguages>

		<!--启用所有的代码分析规则-->
		<AnalysisMode>All</AnalysisMode>

		<!-- 包描述 -->
		<Description>RTU Solution - 工业通讯协议库</Description>

		<!-- 包标签 -->
		<PackageTags>RTU;DLT645;Modbus;Industrial;IoT;Communication;Protocol</PackageTags>

		<!-- 许可证表达式 -->
		<PackageLicenseExpression>MIT</PackageLicenseExpression>

		<!-- 许可证接受 -->
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>

		<!-- 设置警告 -->
		<NoWarn>$(NoWarn);1591;0436</NoWarn>

		<!-- 生成文档文件 -->
		<GenerateDocumentationFile>true</GenerateDocumentationFile>

		<!-- 允许重复的发布输出文件 -->
		<ErrorOnDuplicatePublishOutputFiles>false</ErrorOnDuplicatePublishOutputFiles>
	</PropertyGroup>
</Project>